{
    "manifest_version": 2,

    "name": "Getting started example",
    "description": "This extension shows a Google Image search result for the current page",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Click to order tabs"
    },
    "permissions": [
        "tabs"
    ],
    "background": {
        "scripts": ["background.js", "lodash.core.min.js"]
    }
}