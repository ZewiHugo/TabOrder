{
    "manifest_version": 2,

    "name": "Getting started example",
    "description": "This extension shows a Google Image search result for the current page",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "click to manage tab",
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "tabs"
    ],
    "background": {
        "page": "bg.html"
    }
}